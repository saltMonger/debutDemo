function _update()
  t -= 1
  stim += 0.01
  if(t % 4 == 0) then
    a = 1
    colr = 8
  elseif(t % 4 == 1)then
    a = 2
    colr = 7
  elseif(t % 4 == 2) then
    a = 1
    colr = 10
  else
    a = 2
    colr = 9
  end
  
  movevec = {cos(stim) / 20,0,0}
  
  cube = rotate_shape(cube,a,0.01)
  cube = translate_shape(cube, movevec)
  
  
  ship = rotate_shape(ship,2,cos(stim)*0.005)
  movevec2 = {0,0,sin(stim*4)*0.05}
  --ship = rotate_shape(ship,1,sin(stim*4)*0.01)
  ship = translate_shape(ship,movevec2)    
  
  if(demosw == 2) then
    if(pl.x < 68)then
      if(t%5 == 1) then
        pl.x += 0.5
        --pl.d = (pl.d+0.02)%1
      end
    else
      if(not turnb) then
        ship = rotate_shape(ship,3,0.25)
        turnb = true
      end
      if(t%5 == 1) then
        pl.y += 0.5
      end
    end
  end
  
  if (t == -500) then
    demosw = 2
  elseif(t == -1000) then
    demosw = 3
  elseif(t == -1500) then
    demosw = 4
  end
  
  --starfield stuff
  for x=0,10,1 do
    new_star()
  end
  update_stars()
  start+=0.02
end

function _draw()
  cls()
  if(demosw == 4) then
    draw_shape(cube,colr)
    checkerboard(gridy1,gridy2,gridy3)
    scroll(greetz,gsize,xpos,ypos,0)
    xpos = xpos - 1
    --dear lord optimize this shite
    if(gridy1 < 98) then
      gridy1 = 128
    elseif(gridy1 < 100) then
      gridy1 -= 0.15
    elseif(gridy1 < 104) then
      gridy1 -= 0.25
    elseif(gridy1 < 112) then
      gridy1 -= 0.333
    else
      gridy1 -= 0.5
    end
    
    
    if(gridy2 < 98) then
      gridy2 = 128
    elseif(gridy2 < 100) then
      gridy2 -= 0.15
    elseif(gridy2 < 104) then
      gridy2 -= 0.25
    elseif(gridy2 < 112) then
      gridy2 -= 0.333
    else
      gridy2 -= 0.5
    end
    
    if(gridy3 < 98) then
      gridy3 = 128
    elseif(gridy3 < 100) then
      gridy3 -= 0.15
    elseif(gridy3 < 104) then
      gridy3 -= 0.25
    elseif(gridy3 < 112) then
      gridy3 -= 0.333
    else
      gridy3 -= 0.5
    end
  elseif(demosw == 2) then
        rectfill(0,0,127,127,9)
    -- test block
      --rectfill(0,0,127,80-pl.x * 2 % 16,10)
      --rectfill(0,0,127,64-pl.x * 2 % 16,12)
      --rectfill(0,0,127,48-pl.x * 2 % 16,10)
      --rectfill(0,0,127,32-pl.x * 2 % 16,12)
      --rectfill(0,0,127,16-pl.x * 2 % 16,10)
    

    
    if(not turnb) then
      if(ray1 < 0) then
        ray1 = 64
      end
      if(ray2 < 0) then
        ray2 = 64
      end
      if(ray3 < 0 ) then
        ray3 = 64
      end
      
      ray1 -= 0.5
      ray2 -= 0.5
      ray3 -= 0.5
      
      line(0,ray1,127,ray1,5)
      line(0,ray2,127,ray2,5)
      line(0,ray3,127,ray3,5)
    else
      --don't do anything!
    end  
    
    draw_3d()

    draw_shape2(ship,8)
  elseif(demosw == 3) then
    map(3,8,20,25,11,9)
    onion(64+16*sin(tim),64+16*cos(tim),circrad,6,20)
    onion(64+4*cos(tim2*2),64+4*sin(tim2*2),circrad,8,20)
    onion(64-8*cos(tim*4),64-8*sin(tim*4),circrad,10,20)
    movecirc()
    tim+=0.01
    tim2+=0.01
  elseif(demosw == 1) then
   drawstar()
  end 
end

