end


function rotate_shape(s,a,r)
  ns = {{},s[2]}
  for p in all(s[1]) do
    add(ns[1],rotate_point(p,a,r))
  end
  return ns
end

function rotate_point(p,a,r)
  if a==1 then
    x,y,z = 3,2,1
  elseif a==2 then
    x,y,z = 1,3,2
  elseif a==3 then
    x,y,z = 1,2,3
  end
  --good old rotation matrix
  nx = cos(r)*(p[x]) - sin(r) * (p[y])
  ny = sin(r)*(p[x]) + cos(r) * (p[y])
  np = {}
  np[x] = nx
  np[y] = ny
  np[z] = p[z]
  return np
 end

function translate_shape(s,t)
  ns = {{},s[2]}
  for p in all(s[1]) do
    add(ns[1],{p[1]+t[1],p[2]+t[2],p[3]+t[3]})
  end
  return ns
end


