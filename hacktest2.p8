 s_count+=.001
 add(stars,s)
end

function update_stars()
 r=(r+0.01)%1
 m=(m+0.001)%1
 for s in all(stars) do
  s.z+=s.z*0.11
  s.s+=0.2
  s.c=fc[min(flr(s.s),3)]
  s.xs+=sin(m)*0.02
  s.ys+=cos(m)*0.02
  s.x=((sin(s.a+cos(r)*0.09)+s.xs)*s.d)*s.z
  s.y=((cos(s.a+cos(r)*0.09)+s.ys)*s.d)*s.z
  if(s.z>10)then
   del(stars,s)
   s_count-=1`
  end
 end

